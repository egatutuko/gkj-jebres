<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GKJJebres" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#00452e</item>
        <item name="colorPrimaryVariant">#00452e</item>
        <item name="colorOnPrimary">#00452e</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#00452e</item>
        <item name="colorSecondaryVariant">#00452e</item>
        <item name="colorOnSecondary">#00452e</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="drawerArrowStyle">@style/BackArrowStyle</item>
    </style>

    <style name="BackArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="color">@android:color/white</item>
    </style>

    <style name="Theme.GKJJebres.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.GKJJebres.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.GKJJebres.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>